{
  "workbench.statusBar.visible": true,
  "workbench.colorTheme": "GitHub Dark Dimmed",
  "workbench.colorCustomizations": {
    "[Material Theme High Contrast]": {
      "editor.background": "#0f1a1e",
      "sideBar.background": "#121d21",
      "panel.background": "#121d21",
      "activityBar.background": "#121d21",
      "tab.inactiveBackground": "#121d21",
      "tab.activeBackground": "#121d21",
      "editorGroupHeader.tabsBackground": "#121d21",
      "breadcrumb.background": "#121d21",
      "editorGroupHeader.tabsBorder": "#091214",
      "editorGroup.dropBackground": "#0a1215",
      "sideBar.dropBackground": "#0a1215",
      "list.dropBackground": "#0a1215"
    }
  },
  "editor.suggestSelection": "first",
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.fontFamily": "Menlo for Powerline",
  "workbench.iconTheme": "material-icon-theme",
  "editor.tabSize": 2,
  "workbench.settings.editor": "json",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "editor.formatOnPaste": true,
  "editor.minimap.enabled": false,
  "tailwindCSS.emmetCompletions": true,
  "headwind.prependCustomClasses": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "debug.onTaskErrors": "debugAnyway",
  "breadcrumbs.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "python.formatting.provider": "black",
  "python.linting.pylintArgs": [
    "--disable=django-not-configured",
    "--django-settings-module=<mainapp>.settings"
  ],
  "prettier.enable": true,
  "files.autoSave": "onFocusChange",
  "[python]": {
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "editor.formatOnPaste": false,
    "editor.codeActionsOnSave": {
      "python.sortImports": "explicit"
    },
    "editor.formatOnType": true
  },
  "[vue]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never"
    }
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never"
    }
  },
  "[jsx-tags]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never"
    }
  },
  "[cpp]": {
    "editor.wordBasedSuggestions": "off",
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true,
    "editor.formatOnSave": false
  },
  "errorLens.exclude": [
    "\"tabler\": Unknown word\\.",
    "\"mantine\": Unknown word\\.",
    "\"decamelized\": Unknown word\\.",
    "\"decamelize\": Unknown word\\.",
    "\"Gtin\": Unknown word\\."
  ],
  "errorLens.excludeBySource": ["eslint(@typescript-eslint/no-unused-vars)"],
  "cSpell.userWords": [
    "Barcodes",
    "Gtin",
    "minio",
    "prepick",
    "Prepicked",
    "Prepicking",
    "storeganizer",
    "syncpack",
    "Unscannable",
    "websockets"
  ],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "[jsx-tags][typescript][typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never"
    }
  },
  "prettify-ts.viewNestedTypes": true,
  "window.zoomLevel": 1,
  "makefile.configureOnOpen": true
}
